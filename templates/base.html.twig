<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Quai Antique</title>
    <link rel="stylesheet" href="{{ asset('assets/css/style.css') }}">
    <link rel="shortcut icon" href="{{ asset('assets/img/logo_onglet.png') }}" />
</head>
<body>

<header class="header">
    {% block header %}
        <a href="{{ path('home') }}"><img src="{{ asset('/assets/img/Quai_antique.png')}}" alt="logo" class="logo"></a>
        <h1 class="title">Quai antique</h1>
        <div class="buttons">
            <button type="button" class="carte" onclick="window.location.href='{{ path('menu') }}';">Carte</button>
            <button type="button" class="connexion">Connexion</button>
        </div>
    {% endblock header %}
</header>

<main>
    {% block main %}
    
    {% endblock main %}
</main>

<footer>
    {% block footer %}
    {% endblock footer %}
</footer>

{{ encore_entry_script_tags('app') }}
</body>
</html>
